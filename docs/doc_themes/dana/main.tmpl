          &rarr; module <a href="./${cleansedmodulename}.html" title="$modulename">$modulename</a>
          #if $classname#
            &rarr; class $classname
          #end if#
          #if $filename#
            &rarr; file $filename
          #end if#
        #end if
      </div>
    </div> <!--! end of #sub-hd -->
    
    <div class="clear"></div>
    

    <!-- SIDEBAR NAVIGATION -->
